import java.util.*;
class HelloWorld {
    public static void main(String[] args) {
        Scanner sc = new Scanner(System.in);
        String op = sc.nextLine();
        int a = sc.nextInt();
        int b = sc.nextInt();
        switch(op){
        case "+":    System.out.println(a+b);
        break;
        case "-":    System.out.println(a-b);
        break;
        case "*":    System.out.println(a*b);
        break;
        case "/":    System.out.println(a/b);
        break;
        default :    System.out.println("invalid operator");
        
        }
    }
}
